			$(document).ready(function() {
			//	initPageConent("page1");	
			//风采展示，团队项目，个人项目按钮区的动作			
				$("#content_title").find("a").click(function(){
				        //jquary方法：.addClass(className)该方法用于给指定对象追加class属性，但是不会删除原来属性
						$(".showT").addClass("showNone");
						//jquary方法：.removeClass(className)该方法用于指定对象删除class属性
						$("#content_title").find("a").removeClass("title_on");
						$(this).addClass("title_on");
						//jquary方法：attr(属性名)，获取属性的值
						//attr(属性名, 属性值),设置属性的值
						//本例中取得rel的属性，三个栏目分别设置成了page1,page2,page3，对应了三个div的class分别为page1,page2,page3
						var relValue = $(this).attr("rel");
						//让选中栏目的内容表示出来
						$("#"+relValue).removeClass("showNone");
						initPageConent(relValue);
					});
				//jquery方法：相册展示功能
				var galleryAdv = $('#content_image1').galleriffic('#thumbs', {
					delay:                  2000,
					//#thumbs的div种展示多少个图片，其中所有的li的class必须是"thumb"
					numThumbs:              5,
					preloadAhead:           10,
					enableTopPager:         true,
					enableBottomPager:      false,
					imageContainerSel:      '#slideshow1',
					controlsContainerSel:   '#controls',
					captionContainerSel:    '#caption',
					loadingContainerSel:    '#loading',
					renderSSControls:       true,
					renderNavControls:      true,
					playLinkText:           '播放',
					pauseLinkText:          '暂停',

			//jquary方法:.css 动态这顶css某个属性的值
			$('#thumbs ul.thumbs li').css('opacity', onMouseOutOpacity)
				.hover(
					//li获得焦点动作
					function () {
						//这里的this表示opacity的属性
						//jquary方法:.fadeTo(speed,最终透明度,回调函数)，渐入淡出效果，使之不透明
						//jquary方法:.fadeOut(speed,回调函数)，渐入淡出效果，使之透明
						$(this).not('.selected').fadeTo('fast', 1.0);
					}, 
					//li失去焦点动作
					function () {
						$(this).not('.selected').fadeTo('fast', onMouseOutOpacity);
					}
				);
				
				//strValue：选中栏目的rel值(page1,page2或者page3)
				function initPageConent(strValue){
					if(strValue=="page1"){
						return true;	
					}
					var nowtime = new Date();
					//jquary的ajax写法，无页面刷新调用其他页面逻辑
					$.ajax({
						type:"post",
						url:strValue+".jsp",
						dataType:"html",
						data:"random="+nowtime,
						success:function(htmlContent) {
						//$(aaa).html(bbb)改变被选元素中的html(innerHTML)
						//$(aaa).append(bbb)向被选元素中追加html(innerHTML)
						//$(aaa).prepend(bbb)向被选元素中追加预设的bbb的html(innerHTML)
						//$(aaa).after()向被选元素之后追加html(innerHTML)
						//$(aaa).before()向被选元素之前追加html(innerHTML)
							$("#"+strValue).html(htmlContent);
						},
						error:function() {
						//这句话其实是没用的
							alert("error");
						}
					});
					//jquary方法.bind，将被选元素添加事件
					jQuery("#"+strValue).bind("keydown",function(event){
						if(event.keyCode==13){
							logon(strValue);
						}
					})
				}
				
				function logon(strValue){
					if(strValue=="page1"){
						return true;	
					}
					var UserID = $("#"+strValue+" input[name=UserID]").val();
					var Password = $("#"+strValue+" input[name=Password]").val();;
					var Rand = $("#"+strValue+" input[name=Rand]").val();;
					var nowtime = new Date();
					$.ajax({
						type:"post",
						url:"../passport/login.jsp",
						dataType:"html",
						data:"random="+nowtime+"&UserCode="+UserID+"&Password="+Password+"&Rand="+Rand,
						success:function(htmlContent) {
							if(htmlContent=="correct"){
								initPageConent(strValue);
							}else{
								changeImage();

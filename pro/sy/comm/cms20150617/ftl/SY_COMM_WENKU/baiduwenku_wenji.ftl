<!--------------评分星的js--开始----------------------->
<script>
	//动态评分的js
	jQuery(document).ready(function(){
	
			/**************给文辑打分的JS*********************/
			var spans = jQuery("#rateStar span");
			jQuery(spans).click(function(){
				var num = jQuery(this).attr("num");
				for(var i=0; i<num; i++){
					var temp = i;
					jQuery(spans[i]).removeClass("ic-star-b-off").addClass("ic-star-b-on");
				}
				jQuery("#rateTip").html("").html("感谢您的评价！");
				jQuery("#rateScore").html("").html(num+".0");
				jQuery(spans).unbind("hover").unbind("click");


				//调用后台的DoclistServ.java中的voteScore方法打分
				var doclistId = jQuery("#list_id").html();
				var score = num ;
				var param = {};
				param["VOTE_SCORE"] = score ;
				param["_PK_"] = doclistId ;
				var result = FireFly.doAct("SY_COMM_WENKU_DOCLIST", "voteScore", param, true);
				//{counter=3, _MSG_=OK,评分成功, avg=2.0}
				var counter = result["counter"] ;
				//var msg = result["_MSG_"] ;
				var avg = result["avg"] ;
				jQuery("#wenji_stars").html("").html(avg);
				jQuery("#count_num").html("").html(counter);
				wenji_score();
			});
		
		
		
		/**************判断用户是否大过分的JS*********************/
		//如果当前用户有过评分记录，将显示过去的评分记录
		var doclistId = jQuery("#list_id").html();
		var param = {};
		param["_PK_"] = doclistId ;
		var if_voted = FireFly.doAct("SY_COMM_WENKU_DOCLIST", "show", param, true);
		if(if_voted["VOTED"] == 1){
			jQuery(spans).unbind("hover").unbind("click");
			//已经打分
			var num = parseInt(if_voted["VOTED_SCORE"]);
			var spans = jQuery("#rateStar span");
			for(var i=0; i<num; i++){
				var temp = i;
				jQuery("#rateTip").html("").html("感谢您的评分！");
				jQuery(spans[i]).removeClass("ic-star-b-off").addClass("ic-star-b-on");
				jQuery("#rateScore").html("").html(num+".0");
			}
		}else{
			//没有打分
			/**************动态的增加星数的JS*********************/
			//如果当前用户没有评过分，将显示动态评分
		var spans = jQuery("#rateStar span");
		jQuery(spans).hover(
			function(){
				var num = jQuery(this).attr("num");

<link rel="stylesheet" type="text/css" href="/sc/comm/carlimit/css/carlimit.css"/>
<script type="text/javascript" src="/sc/comm/carlimit/js/carlimit.js"></script> 
<div class='portal-box'>
	<div class='portal-box-title'>
    	<span class="portal-box-title-pre"></span>
        <span class="portal-box-title-label">车辆限行</span>
		<span class='portal-box-title-fix'></span>
    </div>
	<div class="carlimit-wrapper" style="height:${height}">
	  <div class="carlimit-group clearfix">
	    <div class="carlimit-today">
	      <div class="carlimit-title">今天<span></span></div>
	      <div class="carlimit-num"></div>
	    </div>
	    <span class="carlimit-line"></span>
	    <div class="carlimit-tomorrow">
	      <div class="carlimit-title">明天<span></span></div>
	      <div class="carlimit-num"></div>
	    </div>
	  </div>
	  <div class="carlimit-seperator">(法定节假日不限行)</div>
	  <div class="carlimit-group clearfix full-week"></div>
	</div>
</div>
